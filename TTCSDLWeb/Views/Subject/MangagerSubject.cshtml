@model IEnumerable<TTCSDLWeb.Models.DTO.Subject>

@{
    ViewBag.Title = "MangagerSubject";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>MangagerSubject</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Mã Môn
        </th>
        <th>
            Mã hiển thị
        </th>
        <th>
            Tên
        </th>
        <th>
            Số tín
        </th>
        <th>
            Số Tiết
        </th>
        <th>
            Lý Thuyết
        </th>
        <th>
            Bài Tập
        </th>
        <th>
            thảo luận
        </th>
        <th>
            Thực Hành
        </th>
        <th>
            HT Thi
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.codeview)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.numberofcredit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.numberoflesson)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.numberoftheory)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.numberofexercise)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.numberofdiscussion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.numberofpractive)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.examform)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { item.code }) |
            @Html.ActionLink("Delete", "Delete", new { item.code })
        </td>
    </tr>
}

</table>
